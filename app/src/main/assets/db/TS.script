CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE PUBLIC.USERS(uid VARCHAR(50) NOT NULL, user_name VARCHAR(30), date_registered TIMESTAMP NOT NULL, password_hash VARCHAR(64) NOT NULL, PRIMARY KEY(uid))
CREATE MEMORY TABLE PUBLIC.EVENTS(eid INTEGER NOT NULL, event_name VARCHAR(30) NOT NULL, date_created TIMESTAMP NOT NULL, description VARCHAR(100), target_finish_time TIMESTAMP, is_favorite INTEGER NOT NULL, PRIMARY KEY(eid))
CREATE MEMORY TABLE PUBLIC.LABELS(lid INTEGER NOT NULL, label_name VARCHAR(16) NOT NULL, PRIMARY KEY(lid))
CREATE MEMORY TABLE PUBLIC.USERSEVENTS(uid VARCHAR(50) NOT NULL, eid INTEGER NOT NULL, CONSTRAINT CONSTRAINT1 PRIMARY KEY(uid, eid), CONSTRAINT CONSTRAINT2 FOREIGN KEY(uid) REFERENCES USERS(uid), CONSTRAINT CONSTRAINT3 FOREIGN KEY(eid) REFERENCES EVENTS(eid))
CREATE MEMORY TABLE PUBLIC.EVENTSLABELS(eid INTEGER NOT NULL, lid INTEGER NOT NULL, CONSTRAINT CONSTRAINT4 PRIMARY KEY(eid, lid), CONSTRAINT CONSTRAINT5 FOREIGN KEY(eid) REFERENCES EVENTS(eid), CONSTRAINT CONSTRAINT6 FOREIGN KEY(lid) REFERENCES LABELS(lid))
